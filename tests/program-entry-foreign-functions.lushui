@(location "../libs/core/core")
module core

use core.text.(concat Text)
use core.nat.(self Nat)

base: Nat = 1034032

name: Text = concat "Jane#" (nat.display (nat.add 1 base))

message: Text = concat (concat "Hello, " name) "."

;;;;;;;;;;;;;;;;;;;;;
;;; PROGRAM ENTRY ;;;
;;;;;;;;;;;;;;;;;;;;;

main: Text =
    message
