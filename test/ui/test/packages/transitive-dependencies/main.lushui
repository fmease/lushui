;;; TEST auxiliary packages/transitive-dependencies

use extern.core.(type.Type duple.Duple nat.(Nat ==) bool.Bool)
use extern.alpha.(bet g-one jamma)
use extern.gamma.((G as The-G) g1 gam)

v0: Duple Nat Nat = bet
v1: The-G = g-one
v2: Bool = == gam jamma

use extern.core.(tuple.Tuple list.List)

main: Tuple
    (List.prepend Type (Duple Nat Nat)
    (List.prepend Type The-G
    (List.prepend Type Bool
    (List.empty Type))))
    =
    Tuple.prepend
        (Duple Nat Nat)
        (List.prepend Type The-G (List.prepend Type Bool (List.empty Type)))
        v0
        (Tuple.prepend
            The-G
            (List.prepend Type Bool (List.empty Type))
            v1
            (Tuple.prepend
                Bool
                (List.empty Type)
                v2
                Tuple.empty))
