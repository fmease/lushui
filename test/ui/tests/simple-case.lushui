use extern.core.(
    bool.(self Bool false true)
    duple.(Duple duple)
    nat.(self Nat))

main: Nat = factorial 50
;;; main: Nat = factorial 100 ;;; @Note overflows stack :/ we need to optimize

factorial (n: Nat): Nat =
    case n of
        0 => 1
        1 => 1
        \n => nat.multiply n (factorial (nat.-? n 1))
