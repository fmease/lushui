use core.nat.Nat
use core.nat

;; main: Nat = factorial 6
;; main: Nat = factorial 10
;; main: Nat = factorial 15
main: Nat = factorial 50
;; main: Nat = factorial 100 ;; @Note overflows stack :/ we need to optimize

factorial (n: Nat): Nat =
    case n of
        0 => 1
        1 => 1
        \n => nat.multiply n (factorial (nat.panicking-subtract n 1))
