;; Homogeneous lists with a fixed length and related functions.
module

use crate.nat.(Nat +)

@public
data Vector (n: Nat) (A: Type): Type of
    empty (A: Type): Vector 0 A
    ;;; @Task rename "cons" to something more descriptive
    cons (n: Nat) (A: Type): A -> Vector n A -> Vector (+ n 1) A

@public
use Vector.(empty cons)
