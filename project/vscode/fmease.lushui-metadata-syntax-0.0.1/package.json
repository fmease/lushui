{
    "name": "lushui-metadata-syntax",
    "displayName": "Lushui Metadata Syntax",
    "description": "Lushui metadata syntax highlighting",
    "publisher": "fmease",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.22.0"
    },
    "contributes": {
        "languages": [
            {
                "id": "lushui-metadata",
                "extensions": [
                    ".metadata"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "lushui-metadata",
                "scopeName": "source.lushui-metadata",
                "path": "./syntaxes/lushui-metadata.tmLanguage.json"
            }
        ]
    }
}
