;; Homogeneous lists with a fixed length and related functions.
module

use topmost.type.Type
use topmost.nat.(Nat +)

@public @known
data Vector (n: Nat) (A: Type): Type of
    empty (A: Type): Vector 0 A
    prepend (n: Nat) (A: Type): A -> Vector n A -> Vector (+ n 1) A

@public
use Vector.(empty prepend)
